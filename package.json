{
  "name": "effect-start",
  "version": "0.28.0",
  "license": "MIT",
  "files": [
    "src/",
    "!src/**/*.test.ts",
    "!src/**/*.test.tsx",
    "!src/tower",
    "dist/",
    "README.md",
    "package.json"
  ],
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./bun": {
      "source": "./src/bun/index.ts",
      "default": "./dist/bun/index.js"
    },
    "./client": {
      "source": "./src/client/index.ts",
      "default": "./dist/client/index.js"
    },
    "./testing": {
      "source": "./src/testing/index.ts",
      "default": "./dist/testing/index.js"
    },
    "./tower": {
      "source": "./src/tower/index.ts",
      "default": "./dist/tower/index.js"
    },
    "./sql": {
      "source": "./src/sql/index.ts",
      "default": "./dist/sql/index.js"
    },
    "./sql/*": {
      "source": "./src/sql/*/index.ts",
      "default": "./dist/sql/*/index.js"
    },
    "./x/*": {
      "source": "./src/x/*/index.ts",
      "default": "./dist/x/*/index.js"
    },
    "./x/tailwind/plugin": {
      "source": "./src/x/tailwind/plugin.ts",
      "default": "./dist/x/tailwind/plugin.js"
    },
    "./System": {
      "source": "./src/System.ts",
      "default": "./dist/System.js"
    },
    "./Unique": {
      "source": "./src/Unique.ts",
      "default": "./dist/Unique.js"
    },
    "./FileRouter": {
      "source": "./src/FileRouter.ts",
      "default": "./dist/FileRouter.js"
    },
    "./GlobalLayer": {
      "source": "./src/GlobalLayer.ts",
      "default": "./dist/GlobalLayer.js"
    },
    "./FileSystem": {
      "source": "./src/FileSystem.ts",
      "default": "./dist/FileSystem.js"
    },
    "./ChildProcess": {
      "source": "./src/ChildProcess.ts",
      "default": "./dist/ChildProcess.js"
    },
    "./experimental": {
      "source": "./src/experimental/index.ts",
      "default": "./dist/experimental/index.js"
    },
    "./client/assets.d.ts": "./src/assets.d.ts",
    "./hyper": {
      "source": "./src/hyper/index.ts",
      "default": "./dist/hyper/index.js"
    },
    "./jsx-runtime": {
      "source": "./src/hyper/jsx-runtime.ts",
      "default": "./dist/hyper/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "source": "./src/hyper/jsx-runtime.ts",
      "default": "./dist/hyper/jsx-runtime.js"
    },
    "./datastar": {
      "source": "./src/datastar/index.ts",
      "default": "./dist/datastar/index.js"
    },
    "./lint/plugin": {
      "source": "./src/lint/plugin.js",
      "default": "./dist/lint/plugin.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "format": "bunx oxfmt",
    "lint": "oxlint --import-plugin --tsconfig tsconfig.json src/",
    "test": "bun test ./src/",
    "test-sql": "bun add --no-save mssql @types/mssql postgres && TEST_SQL=1 bun test ./src/sql/",
    "bump": "bun scripts/bump",
    "prepublishOnly": "bun run build",
    "deploy": "bunx --bun npm publish --access public"
  },
  "devDependencies": {
    "@libsql/client": "^0.17.0",
    "@types/bun": "^1.3.4",
    "@types/jsdom": "^27.0.0",
    "@typescript/native-preview": "^7.0.0-dev.20260217.1",
    "effect-memfs": "^0.8.0",
    "jsdom": "^28.0.0",
    "mssql": "^12.2.0",
    "oxfmt": "latest",
    "oxlint": "^1.43.0",
    "postgres": "^3.4.8",
    "tailwindcss": "^4.1.18",
    "ts-morph": "^27.0.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@libsql/client": ">=0.17.0",
    "effect": "^3.19.11",
    "mssql": ">=12.2.0"
  },
  "peerDependenciesMeta": {
    "@libsql/client": {
      "optional": true
    },
    "mssql": {
      "optional": true
    }
  }
}

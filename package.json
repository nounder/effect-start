{
  "name": "effect-start",
  "version": "0.26.0",
  "license": "MIT",
  "files": [
    "src/",
    "!src/**/*.test.ts",
    "!src/**/*.test.tsx",
    "!src/tower",
    "dist/",
    "README.md",
    "package.json"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./bun": "./src/bun/index.ts",
    "./client": "./src/client/index.ts",
    "./testing": "./src/testing/index.ts",
    "./tower": "./src/tower/index.ts",
    "./sql": "./src/sql/index.ts",
    "./sql/*": "./src/sql/*/index.ts",
    "./x/*": "./src/x/*/index.ts",
    "./x/tailwind/plugin": "./src/x/tailwind/plugin.ts",
    "./System": "./src/System.ts",
    "./Unique": "./src/Unique.ts",
    "./FileRouter": "./src/FileRouter.ts",
    "./FileSystem": "./src/FileSystem.ts",
    "./experimental": "./src/experimental/index.ts",
    "./client/assets.d.ts": "./src/assets.d.ts",
    "./hyper": "./src/hyper/index.ts",
    "./jsx-runtime": "./src/hyper/jsx-runtime.ts",
    "./jsx-dev-runtime": "./src/hyper/jsx-runtime.ts",
    "./datastar": "./src/datastar/index.ts",
    "./lint/plugin": "./src/lint/plugin.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "bunx --bun tsc -p tsconfig.build.json",
    "format": "bunx oxfmt",
    "lint": "oxlint --import-plugin --tsconfig tsconfig.json src/",
    "test": "bun test ./src/",
    "test-sql": "bun add --no-save mssql @types/mssql postgres && TEST_SQL=1 bun test ./src/sql/",
    "deploy": "bunx --bun npm publish --access public"
  },
  "devDependencies": {
    "@types/bun": "^1.3.4",
    "@types/jsdom": "^27.0.0",
    "effect-memfs": "^0.8.0",
    "jsdom": "^28.0.0",
    "oxfmt": "latest",
    "oxlint": "^1.43.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "effect": "^3.19.11"
  }
}
